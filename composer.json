{
  "name": "bhekor/laravel-geetest",
  "description": "Geetest Package for Laravel",
  "type": "library",
  "keywords": [
    "Geetest",
    "Laravel",
    "Bhekor"
  ],
  "license": "MIT",
  "support": {
    "email": "adeoluibidapo@gmail.com"
  },
  "authors": [
    {
      "name": "Ibidapo Adeolu",
      "email": "adeoluibidapo@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "illuminate/support": ">=7.0",
    "illuminate/routing": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Bhekor\\LaravelGeetest\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bhekor\\LaravelGeetest\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
        "providers": [
            "Bhekor\\LaravelGeetest\\GeetestServiceProvider"
        ],
        "aliases": {
            "Geetest": "Bhekor\\LaravelGeetest\\Geetest"
        }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
